 <h1 class="heading">Edit Page</h1>

 <form class="row g-3 needs-validation" action="/update/<%=viewchat._id %>?_method=PUT" method="POST" style="margin-left: 10rem;margin-right: 5rem;" novalidate >
  <div class="col-md-6">
    <label for="inputEmail4" class="form-label">Title</label>
    <input type="Title" class="form-control"name="title" id="inputEmail4" required value="<%= viewchat.title %>">
  </div>
  <div class="col-md-4">
    <label for="inputPassword4" class="form-label">Price</label>
    <input type="number" class="form-control" id="inputPassword4" name="price" required value="<%= viewchat.price %>" >
  </div>
  <div class="col-10">
    <label for="inputAddress" class="form-label">Description</label>
    <input type="text"  class="form-control" id="inputAddress" placeholder="this is good for family" name="description"required  value="<%=viewchat.description %> ">
  </div>
  <div class="col-10">
    <label for="inputAddress2" class="form-label">Image URL</label>
    <input type="text" class="form-control" id="inputAddress2" value="<%=viewchat.image%>">
  </div>
  <div class="col-md-5">
    <label for="inputCity" class="form-label">City</label>
    <input type="text" class="form-control" id="inputCity" name="location" value="<%=viewchat.location %>">
  </div>
  
  <div class="col-md-5">
    <label for="inputZip" class="form-label">Country</label>
    <input type="text" class="form-control" id="inputZip" name="country" value="<%=viewchat.country%>">
  </div>
  <div class="col-12">
    <div class="form-check">
      <input class="form-check-input" type="checkbox" id="gridCheck">
      <label class="form-check-label" for="gridCheck">
        Agree to terms and Condition
      </label>
    </div>
  </div>
  <div class="col-12">
    <button type="submit" class="btn btn-success">Submit</button>
  </div>
</form>
